        <licenseUrl>https://github.com/Yoshie2000/PlentyChess/blob/main/LICENSE</licenseUrl>
        <packageSourceUrl>https://github.com/n0y/chocolatey-ucichess-engine-plentychess</packageSourceUrl>
        <tags>uci chess</tags>
    </metadata>
    <files>
        <file src="tools\**" target="tools"/>
    </files>
</package>
